{
  "name": "type-flat",
  "version": "0.0.1",
  "description": "TypeScript Type Flattening Tool ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "type-flat": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "type",
    "flatten",
    "cli",
    "ts"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:es": "rollup -c ",
    "release": "npx bumpp --no-push",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "author": "hboot<bobolity@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngd-b/type-flat.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "husky": "^9.1.7",
    "rollup": "^4.52.5",
    "rollup-plugin-copy": "^3.5.0"
  }
}
