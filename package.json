{
  "name": "type-flat",
  "version": "0.2.4",
  "description": "TypeScript Type Flattening Tool ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "type-flat": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": "./dist/cli.js"
  },
  "napi": {
    "binaryName": "type-flat",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-musl"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "type",
    "flatten",
    "cli",
    "ts"
  ],
  "scripts": {
    "test": "ava",
    "build": "tsx scripts/build.ts",
    "build:napi": "npx napi build --platform --release -o pkg",
    "build:es": "rollup -c ",
    "release": "npx bumpp",
    "publish:npm": "tsx scripts/publish.ts"
  },
  "author": "hboot<bobolity@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngd-b/type-flat.git"
  },
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.18.3",
  "browser": false,
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@napi-rs/cli": "^3.4.1",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/node": "^24.9.1",
    "ava": "^6.4.1",
    "rollup": "^4.52.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "ava": {
    "timeout": "2m",
    "workerThreads": false,
    "files": [
      "test-web/**"
    ]
  }
}
