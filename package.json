{
  "name": "type-flat",
  "version": "0.0.1",
  "description": "TypeScript Type Flattening Tool ",
  "main": "dist/type_flat.js",
  "types": "dist/type_flat.d.ts",
  "type": "module",
  "bin": {
    "type-flat": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/type_flat.js",
      "types": "./dist/type_flat.d.ts"
    },
    "./cli": "./dist/cli.js"
  },
  "fils": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "type",
    "flatten",
    "cli",
    "ts"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "release": "npx bumpp --no-push",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "author": "hboot<bobolity@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngd-b/type-flat.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "husky": "^9.1.7"
  }
}
