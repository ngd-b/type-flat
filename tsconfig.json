{
  "compilerOptions": {
    "importHelpers": true,
    "target": "ES2022",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "declaration": false,
    "outDir": "dist",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
  },
  "include": ["web"],
  "exclude":["node_modules/**","dist/**"]
}
